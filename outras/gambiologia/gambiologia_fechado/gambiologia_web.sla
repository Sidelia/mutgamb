            <para/>
            <ITEXT CH="[3] http://jardinagemlibertaria.wordpress.com/"/>
            <para/>
            <ITEXT CH="[4] http://pt.wikipedia.org/wiki/Anarco-primitivismo"/>
            <para/>
            <ITEXT CH="[5] http://pt.wikipedia.org/wiki/Theodore_Kaczynski"/>
            <para/>
